    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@steeze-ui/heroicons": "^2.2.1",
